{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run lint && npm run build && cp src-tauri/target/debug/executables.json src-tauri/target/release/executables.json",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "CPU High Priority",
    "version": "2.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true,
        "execute": true,
        "scope": [{
          "name": "run-cmd",
          "cmd": "cmd.exe",
          "args": true
        }]
      },
      "dialog": {
        "all": false,
        "open": true
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "cpu-high-priority.skt-stk.dev",
      "icon": [
        "icons/icon.png",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "CPU High Priority",
        "width": 800,
        "height": 600,
        "minWidth": 500,
        "minHeight": 375,
        "visible": false
      }
    ]
  }
}
