{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "CPU High Priority",
    "version": "1.2.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true,
        "execute": true,
        "scope": [{
          "name": "run-cmd",
          "cmd": "cmd.exe",
          "args": true
        }]
      },
      "dialog": {
        "all": false,
        "open": true
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "cpu-high-priority.skt-stk.dev",
      "icon": [
        "icons/icon.png",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "CPU High Priority",
        "width": 800,
        "height": 600,
        "minWidth": 500,
        "minHeight": 375,
        "visible": false
      },
      {
        "width": 400,
        "height": 200,
        "title": "CPU High Priority",
        "label": "splashscreen",
        "center": true,
        "decorations": false,
        "alwaysOnTop": true,
        "transparent": true,
        "url": "splashscreen.html"
      }
    ]
  }
}
